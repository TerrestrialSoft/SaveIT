{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "AllowedHosts": "*",
  "GoogleClient": {
    "TokenUrl": "https://oauth2.googleapis.com/token",
    "OAuthUrl": "https://accounts.google.com/o/oauth2/v2/auth",
    "LocalRedirectUrl": "{0}/api/auth/google/callback"
  },
  "TokenCaching": {
    "TokenRetrieveDelaySeconds": 5,
    "CacheEntryLifeTimeSeconds": 900,
    "MaxTokenRetrieveTimeSeconds": 180
  }
}
